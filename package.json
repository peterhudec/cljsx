{
  "name": "cljsx",
  "version": "1.0.0",
  "description": "JSX for Clojure and ClojureScript",
  "devDependencies": {
    "@cljs-oss/module-deps": "^1.1.1",
    "shadow-cljs": "^2.8.25",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "start": "shadow-cljs watch example",
    "lint": "lein lint",
    "clj-test": "lein clj-test",
    "cljs-test": "rm -rf .shadow-cljs && shadow-cljs compile node-test && node target/public/node-test.js",
    "test": "npm run clj-test && npm run cljs-test",
    "check": "npm test && npm run lint",
    "cljs-test:watch": "shadow-cljs watch node-test-dev",
    "fig:build": "lein fig:build",
    "fig": "lein fig",
    "travis": "shadow-cljs compile node-test && node target/public/node-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterhudec/cljsx.git"
